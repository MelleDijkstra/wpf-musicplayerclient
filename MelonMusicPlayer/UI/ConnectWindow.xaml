<Window x:Class="MelonMusicPlayerWPF.UI.ConnectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MelonMusicPlayer" Height="300" ResizeMode="NoResize" Width="366.059" WindowStartupLocation="CenterOwner">
    <Grid>
        <TextBox Name="TbIP" Margin="0,60,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" HorizontalAlignment="Center" Padding="5"/>
        <Button Name="BtnConnect" Content="CONNECT" HorizontalAlignment="Center" Margin="0,50,0,0" Width="120" Height="36" Background="#FF148949" Foreground="White" Click="btnConnect_Click" VerticalAlignment="Center"/>
    </Grid>
</Window>